    <script>
      function toggleDiv(divid)
      {
        let name=divid;
    if(name==='profileSection'){
    document.getElementById('profileSection').style.display='block';
    document.getElementById('editSection').style.display='none';
    document.getElementById('edit').classList.remove('active');
    document.getElementById('profile').classList.add('active');
   }else if(name==='editSection'){
    document.getElementById('profileSection').style.display='none';
    document.getElementById('editSection').style.display='block';
    document.getElementById('profile').classList.remove('active');
    document.getElementById('edit').classList.add('active');
   }
   else{
    document.getElementById('profileSection').style.display='block';
    document.getElementById('editSection').style.display='none';
    document.getElementById('edit').classList.remove('active');
    document.getElementById('profile').classList.add('active');
   }
    } 

    function calling(link){
        let name =link;
        if (name==='dashboard'){
          document.getElementById('dashboard').style.display='block';
          // document.getElementById('sublist').style.display='none';
          document.getElementById('product').style.display='none';
        }
        // else if(name==='sublist'){
        //   document.getElementById('dashboard').style.display='none';
        //   document.getElementById('sublist').style.display='block';
        //   document.getElementById('product').style.display='none';
        // }
        else if(name==='product'){
          document.getElementById('dashboard').style.display='none';
          // document.getElementById('sublist').style.display='none';
          document.getElementById('product').style.display='block';
        }
        else{
          document.getElementById('dashboard').style.display='block';
          // document.getElementById('sublist').style.display='none';
          document.getElementById('product').style.display='none';
        }
      }
    </script>
